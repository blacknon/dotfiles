{
    "window.zoomLevel": 1,
    "go.useLanguageServer": true,
    "files.insertFinalNewline": true,
    "editor.fontSize": 14,
    "editor.wrappingIndent": "same",
    "editor.lineNumbers": "on",
    "editor.matchBrackets": true,
    "files.trimTrailingWhitespace": true,
    "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~? 　、。",
    "editor.wordWrap": "on",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "materialTheme.accent": "Cyan",
    "guides.enabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "explorer.confirmDelete": false,
    "editor.fontFamily": "SourceHanCodeJP-Regular, Menlo, Monaco, 'Courier New', monospace",
    "debug.console.fontSize": 14,
    "rust.mode": "legacy",
    "rust.formatOnSave": true,
    "rust.rustup": "/Users/blacknon/.cargo/bin/rustup",
    "rust.rustfmtPath": "/Users/blacknon/.cargo/bin/rustfmt",
    "terminal.integrated.rendererType": "dom",
    "git.ignoreMissingGitWarning": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "go.playground": {
        "openbrowser": true,
        "share": true,
        "run": true
    },
    "python.linting.flake8Args": [
        "--ignore=E501"
    ]
}
